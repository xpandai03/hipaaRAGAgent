# Production Environment Variables for GPT-OSS
# Update EXTERNAL_IP with your actual GCP instance IP after deployment

# GPT-OSS API endpoint (will be updated with actual IP)
NEXT_PUBLIC_API_URL=http://EXTERNAL_IP:8080

# Tenant Router API
NEXT_PUBLIC_TENANT_API_URL=http://EXTERNAL_IP:8080/v1/chat/completions

# Default tenant (can be amanda, robbie, emmer, or default)
NEXT_PUBLIC_DEFAULT_TENANT=amanda

# Enable tenant selection UI
NEXT_PUBLIC_ENABLE_TENANT_SELECTION=true

# WebSocket for streaming (if needed)
NEXT_PUBLIC_WS_URL=ws://EXTERNAL_IP:8080/ws

# Monitoring endpoints
NEXT_PUBLIC_MONITORING_URL=http://EXTERNAL_IP:9090
NEXT_PUBLIC_GRAFANA_URL=http://EXTERNAL_IP:3000